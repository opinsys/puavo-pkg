#!/bin/sh

set -eu

command=$1
shift

case "${command}" in
    download|install|activate|deactivate|uninstall|purge|list)
        "/usr/local/lib/puavo-restricted-package-helper/scripts/${command}" "$@"
        ;;
    *)
        echo "E: unknown command '${command}'" >&2
        exit 1
esac

exit 0

